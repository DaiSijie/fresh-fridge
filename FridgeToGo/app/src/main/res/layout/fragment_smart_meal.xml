        android:layout_marginStart="48dp"
        android:id="@+id/button21" />

    <Button
        android:text="Beurk!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/button21"
        android:layout_toRightOf="@+id/button21"
        android:layout_toEndOf="@+id/button21"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:id="@+id/button22" />

    <Button
        android:text="I like!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView3"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="48dp"
        android:id="@+id/button31" />

    <Button
        android:text="Beurk!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/button31"
        android:layout_toEndOf="@+id/button31"
        android:layout_alignBaseline="@id/button31"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:id="@+id/button32" />

</RelativeLayout>
